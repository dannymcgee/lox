{
	"npmScope": "lox",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"workspace.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tsconfig.base.json": "*",
		"tslint.json": "*",
		".eslintrc.json": "*",
		"nx.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"projects": {
		"spec": {
			"tags": []
		},
		"vscode-lox": {
			"tags": []
		},
		"ts": {
			"tags": []
		},
		"tools": {
			"tags": []
		},
		"cli-tools": {
			"tags": []
		}
	},
	"workspaceLayout": {
		"appsDir": "packages",
		"libsDir": "packages"
	}
}
